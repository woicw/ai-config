{
    "mcpServers": {
        "sequential-thinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "console-ninja": {
            "command": "node",
            "args": [
                "~/.console-ninja/mcp/"
            ]
        },
        "API-Doc": {
            "command": "npx",
            "args": [
                "-y",
                "apifox-mcp-server@latest",
                "--project=4955295"
            ],
            "env": {
                "APIFOX_ACCESS_TOKEN": "<your-token>"
            }
        },
        "@21st-dev/magic": {
            "command": "npx",
            "args": [
                "-y",
                "@21st-dev/magic@latest",
                "API_KEY=\"<your-api-key>\""
            ]
        },
        "shadcn-ui-server": {
            "command": "/Users/woic/github/shadcn-ui-mcp-server/build/index.js"
        },
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp@latest"
            ]
        },
        "chrome-devtools": {
            "command": "npx",
            "args": [
                "-y",
                "chrome-devtools-mcp"
            ]
        }
    }
}